--生成商品搜索数据

Truncate table Product_Search

Go

Insert Into [Product_Search] (
		 [ProductID]
		,[ProductName]
		,[ProductBarcode]
		,[ProductCategory]
		,[ProductBrand]
		,[ParentCategory]
		,[ParentBrand]
		,[MarketPrice]
		,[GoujiuPrice]
		,[Status])
SELECT [ID]
		,[Name]		
		,[Barcode]
		,(select CategoryName from Product_Category where Product_Category.ID = Product.ProductCategoryID)
		,(select BrandName from Product_Brand where Product_Brand.ID = Product.ProductBrandID)
		,(select CategoryName from Product_Category where Product_Category.ID = Product.ParentCategoryID)
		,(select BrandName from Product_Brand where Product_Brand.ID = Product.ParentBrandID)
		,[MarketPrice]
		,[GoujiuPrice]
		,[Status]
	FROM [dbo].[Product] Where IsDelete = 0

Update Product_Search Set [ProductNamePinYin] = LOWER(dbo.fn_GetQuanPin([ProductName])),
[ProductCategoryPinYin] = LOWER(dbo.fn_GetQuanPin([ProductCategory])),
[ProductBrandPinYin] = LOWER(dbo.fn_GetQuanPin([ProductBrand])),
