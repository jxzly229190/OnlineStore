	insert into goujiuwang_v5.[dbo].Product_Comment (ID,ProductID,OrderID,Score,UserID,Content,[Status],IsDelete,CreateTime,[ExtField],[EmployeeID],[AuditTime],[AuditDescription])
		SELECT [ID]
		,[pro_ProductID]as[ProductID]
		,isnull((select [odr_OrderID] From goujiuwang_v4.[dbo].[odr_OrderProduct] where ID =[odr_OrderProductID]),0)as[OrderID]
		,[DescriptionScore]as[Score]
		,[usr_UserID]as[UserID]
		,isnull([Content],'')as[Content]
		,case when [State] = 1 then 2 else 1  end [Status] --状态
		,case when [State]=255 then 255 else 0 end [IsDelete] --删除标识
		,[CreateTime] as[CreateTime]
		,[ExtField] as [ExtField]
		,0 as [EmployeeID] -- 审核人
		,[CreateTime]as [AuditTime] --审核时间
		,'' as [AuditDescription] --审核描述
	FROM goujiuwang_v4.[dbo].[pro_ProductUserComment]
		
