Go

--获取订单的配送服务信息
Update Order_Status_Tracking set ExpressNumber = A.ExpressNumber, Remark=a.remark,MailNo=A.mailNo
From (
Select a.*,case when a.Name is null then '订单已经发货，请注意查收.' else '订单已经发货，配送单位：'+a.Name+'；快递单号:'+a.mailNo end remark
From (
select c.Id as trackingId, a.id orderid,a.OrderCode,a.OrderNumber,a.status,
	substring(c.remark,6,charindex(';',c.remark)-6) ExpressNumber,
	case charindex(';',c.remark,charindex(';',c.remark)+1)
		when 0 then substring(c.remark,charindex('快递单号',c.remark)+5,len(c.remark)-charindex('快递单号',c.remark)-4)
		else substring(c.remark,charindex('快递单号',c.remark)+5,charindex(';',c.remark,charindex(';',c.remark)+1)-charindex('快递单号',c.remark)-5)
		end mailNo,
	d.Name,d.Number
	,a.DeliveryCorporationID
	from [order] a
	left join (select max(id) Id, orderid from Order_Status_Tracking where status=2 group by orderid) b on a.id=b.orderid
	left join Order_Status_Tracking c on b.id=c.id
	Left Join Config_Delivery_Corporation d on d.ID=a.DeliveryCorporationID
	where a.status=2 and c.remark like '快递代码:%' ) a) A
Where A.trackingId = Order_Status_Tracking.ID and A.orderid = Order_Status_Tracking.OrderID


